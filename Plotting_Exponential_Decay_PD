   "source": [
    "tau_rs = []\n",
    "av_scats = []\n",
    "\n",
    "with open('testl3b.txt', 'r') as file:\n",
    "    for line in file:\n",
    "        # Split each line into columns (assuming space-separated)\n",
    "        parts = line.split()\n",
    "        \n",
    "        # Convert and append the values to respective arrays\n",
    "        tau_rs.append(float(parts[0]))\n",
    "        av_scats.append(float(parts[1]))\n",
    "\n",
    "y_model = []\n",
    "for tau in tau_rs:\n",
    "    y = tau + tau*tau/2\n",
    "    y_model.append(y)"
   ]
  },
